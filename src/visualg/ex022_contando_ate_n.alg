algoritmo "Contando até o valor de entrada"
var
    contador, valor : inteiro
inicio

      contador <- 0
      
      escreva("Informe um valor inteiro para o nosso programa contar: ")
      leia(valor)
      
      enquanto (contador <= valor) faça
      
           escreva(contador)
           contador <- contador + 1
      
      fimenquanto
      
fimalgoritmo