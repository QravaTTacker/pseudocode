algoritmo "Imposto"
var
preco, porcentual, total : real
inicio

      escreva("Informe o preço do produto: ")
      leia(preco)
      escreva("Qual foi o porcentual: ")
      leia(porcentual)
      total <- preco + preco*porcentual/100)
      escreval("O valor total será: ", total)

fimalgoritmo